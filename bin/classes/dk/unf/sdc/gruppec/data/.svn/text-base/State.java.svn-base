package dk.unf.sdc.gruppec.data;

public class State {
	public int state;
	public boolean revertOnExit;
	public boolean previousOn;

	public State() {
		state = 2;
		revertOnExit = false;
		previousOn = false;
	}
	
	public State(int setState, boolean revert) {
		state = setState;
		revertOnExit = revert;
		previousOn = false;
	}
}
